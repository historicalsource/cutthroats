	.FUNCT	CONTACT-ESTABLISHED,OCONT,V
	SET	'OCONT,P-CONT
	SET	'P-CONT,TRUE-VALUE
	SET	'P-MERGED,TRUE-VALUE
	CALL	PERFORM,V?TELL,L-WINNER >V
	SET	'P-CONT,OCONT
	SET	'P-MERGED,FALSE-VALUE
	RETURN	V


