	CRLF	
	JUMP	?CND39
?ELS41:	PRINTI	"I can't see "
	CALL	THE?,OBJ
	CALL	DPRINT,OBJ
	PRINTI	" anymore."
	CRLF	
?CND39:	SET	'CLOCK-WAIT,TRUE-VALUE
	RETURN	2
?ELS36:	EQUAL?	L-WINNER,PLAYER /?THN65
