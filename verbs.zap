	PRINT	STACK
	CRLF	
	RETURN	2
?ELS14:	EQUAL?	PTS,FEXIT \?ELS20
	GET	PT,FEXITFCN
	CALL	STACK >RM
	ZERO?	RM /?ELS25
	CALL	GOTO,RM
	RSTACK	
?ELS25:	RETURN	2
?ELS20:	EQUAL?	PTS,CEXIT \?ELS31
	GETB	PT,CEXITFLAG
